{{#if session.isAuthenticated}}
  {{removePanelParam}}
  {{account-subpanel 
    verifiableUsers=verifiableUsers
    hideFilters=(action hideFilters)
    toggleMenu=(action toggleMenu)
  }}
{{else}}
  {{#if (eq panel 'signup')}}
    {{signup-subpanel}}
  {{else}}
    {{login-subpanel 
      showSignupPanel=showSignupPanel
    }}
  {{/if}}
{{/if}}
